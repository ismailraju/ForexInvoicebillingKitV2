<AnchorPane fx:id="MainTabPane" prefHeight="600.0" prefWidth="1320.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.istl.iris.controller.MainTabController">
   <children>
      <HBox alignment="CENTER" prefHeight="52.0" prefWidth="1320.0" spacing="15.0" AnchorPane.topAnchor="0.0">

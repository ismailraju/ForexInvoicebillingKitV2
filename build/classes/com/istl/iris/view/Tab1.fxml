      </HBox>
      <HBox alignment="CENTER" layoutX="665.0" layoutY="80.0" prefHeight="100.0" prefWidth="200.0" style="-fx-border-color: black;" AnchorPane.bottomAnchor="65.0">
         <children>
            <Canvas fx:id="leftEyeCanvas" height="480.0" width="640.0" />
         </children>
      </HBox>
      <HBox layoutY="691.0" prefHeight="60.0" prefWidth="1320.0" spacing="15.0" style="-fx-background-color: #eeeeee;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
           
           
            <Label fx:id="MainTabLblStatus" prefHeight="40.0" prefWidth="1040.0" text="Status">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="MainTabBtnSave" disable="true" mnemonicParsing="false" onAction="#MainTabBtnSaveClicked" prefHeight="40.0" prefWidth="120.0" styleClass="buttons" stylesheets="@../../../../style/fingerprint.css" text="Save">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="MainTabBtnCancel" mnemonicParsing="false" onAction="#MainTabBtnCancelClicked" prefHeight="40.0" prefWidth="120.0" stylesheets="@../../../../style/fingerprint.css" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
               <styleClass>
                  <String fx:value="close_buttons" />
                  <String fx:value="buttons" />
               </styleClass>
            </Button>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>

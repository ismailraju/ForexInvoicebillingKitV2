            <Label prefHeight="81.0" prefWidth="200.0" text="Right Iris Size : ">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="MainTabRightIrisSizelbl" prefHeight="81.0" prefWidth="100.0" text="XXX">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="MainTabDeviceUpSideDownlbl" alignment="CENTER" prefHeight="81.0" prefWidth="300.0" text="Device OK" textFill="#0062ff">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label prefHeight="81.0" prefWidth="200.0" text="Left Iris Size : ">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="MainTabLeftIrisSizelbl" prefHeight="81.0" prefWidth="100.0" text="XXX">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
